- var n = 1

doctype html
html
  head
    title Not Seek
    style
      include style.css
  body 
    h1 Latest Vacancies
    h3 Page Number #{page} of #{pageCount}
    div(class='test-jobs')
      ul
        each job in jobs
         li
            a(href=`/other.html?id=${job.uuid}`) #{job.title}
    a(href=`/nextPage.html?id=${page}`) Page